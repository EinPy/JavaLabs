		SimpleWindow w = new SimpleWindow(width, height, "LineDrawing");

		while (true) {
			// vänta tills användaren klickar på en musknapp
			// rita en linje till den punkt där användaren klickade
			w.waitForEvent();
			int t = w.getEventType();


			
		}
	}
}
